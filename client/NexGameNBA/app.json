{
  "expo": {
    "name": "NexGameNBA",
    "slug": "NexGameNBA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nba-icon2.jpg",
    "scheme": "nexgamenba",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "image": "./assets/images/nba-icon2.jpg",
      "backgroundColor": "#00438C",
      "resizeMode": "cover"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#00438C",
        "foregroundImage": "./assets/images/nba-icon2.jpg",
        "backgroundImage": "./assets/images/nba-icon2.jpg",
        "monochromeImage": "./assets/images/nba-icon2.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.NexGameNBA"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/nba.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nba.png",
          //"imageWidth": 600,
          "resizeMode": "cover",
          "backgroundColor": "#00438C",
          "dark": {
            "backgroundColor": "#00438C"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-xxxxxxxx~xxxxxxxx",
          "iosAppId": "ca-app-pub-xxxxxxxx~xxxxxxxx"
        }
      ],
      "expo-tracking-transparency",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "turboModules": false
    }
  }
}
